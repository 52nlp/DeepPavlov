{
  "dataset_reader": {
    "name": "basic_neural_ranker_reader",
    "data_path": "/media/olga/Data/datasets/squad/preproc"
  },
  "dataset_iterator": {
    "name": "basic_neural_ranker_iterator",
    "seed": 42
  },
  "chainer": {
    "in": [
      "x"
    ],
    "in_y": [
      "y"
    ],
    "pipe": [
      {
        "name": "ranker_encoder",
        "in": ["x"],
        "in_y": ["y"],
        "out": ["y_probas"],
        "save_path": "bnr/model",
        "load_path": "bnr/model"
      }
    ],
    "out": [
      "y_probas"
    ]
  },
  "train": {
    "epochs": 1000,
    "batch_size": 1,
    "metrics": [
      "ranker_recall",
      "accuracy"
    ],
    "validation_patience": 5,
    "val_every_n_epochs": 5,
    "log_every_n_batches": 1000,
    "show_examples": false,
    "test_best": false,
    "validate_best": true,
    "tensorboard_log_dir": "bnr/logs"
  },
  "metadata": {
  }
}
